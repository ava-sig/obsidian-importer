{
	"name": "obsidian-importer",
	"version": "1.6.22",
	"description": "Import notes from other apps like Evernote.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json package.json versions.json",
		"lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"__tests__/**/*.ts\" --fix",
		"check:glyph-header": "node scripts/glyph-header-check.js",
		"ci:pr": "node scripts/run-governance-tool.mjs ts check-pr-body.ts",
		"validate:links": "node scripts/run-governance-tool.mjs ts validate-link-graph.ts",
		"perf:budgets": "node scripts/run-governance-tool.mjs js enforce-budgets.js",
		"fixtures:redact": "node scripts/run-governance-tool.mjs ts redact-recordings.ts",
		"fixtures:generate": "node scripts/run-governance-tool.mjs ts generate-fixtures.ts",
		"test": "bash scripts/test-meta.sh",
		"smoke": "bash scripts/smoke-meta.sh",
		"verify": "npm run lint && npm run build",
		"prepare": "husky install",
		"prune": "git fetch --prune origin && git remote prune origin && git for-each-ref --format='%(refname:short) %(upstream:trackshort)' refs/heads | awk '$2==\"[gone]\"{print $1}' | xargs -r -n1 git branch -D",
		"session:start": "bash scripts/session-start.sh",
		"session:end": "bash scripts/session-end.sh",
		"session:merge": "bash scripts/session-merge.sh",
		"session:cleanup": "bash scripts/session-cleanup.sh"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@microsoft/microsoft-graph-types": "2.38.0",
		"@types/node": "16.6.2",
		"@types/xml-flow": "1.0.1",
		"@typescript-eslint/eslint-plugin": "6.2.1",
		"@typescript-eslint/parser": "6.2.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.25.4",
		"eslint": "8.46.0",
		"husky": "9.1.7",
		"obsidian": "latest",
		"fast-glob": "3.3.2",
		"vitest": "0.34.6",
		"tsx": "4.7.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@joplin/turndown-plugin-gfm": "1.0.62",
		"@zip.js/zip.js": "2.7.60",
		"plain-tag": "0.1.3",
		"protobufjs": "7.2.5",
		"static-params": "0.4.0",
		"xml-flow": "1.0.4"
	}
}
